  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "pm2:dev": "pm2 start ecosystem.config.js",
    "pm2:prod": "npm run build && pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop vidacure-server",
    "pm2:restart": "pm2 restart vidacure-server",
    "pm2:delete": "pm2 delete vidacure-server",
    "pm2:logs": "pm2 logs vidacure-server"
